<?xml version="1.0" encoding="utf-8"?>
<resources>
	<string name="app_name">Cmwrap</string>
	<string name="serviceTagUp">启用CMWAP穿越</string>
	<string name="serviceTagDown">禁用CMWAP穿越</string>
	<string name="serviceBaseTitle">切换基本运行状态</string>
	<string name="serviceBaseComment">此运行状态下，耗电量与未打开时几乎没有差别</string>
	<string name="serviceTagApp">启用CMWAP穿越+基础Google服务</string>
	<string name="serviceAppTitle">切换基本服务状态</string>
	<string name="serviceAppComment">
		此状态将启用Gmail、Gtalk等基本Google服务，由于Gmail等应用程序同步需要，耗电量稍微多于基本运行状态
	</string>
	<string name="serviceTagSuper">启用所有服务</string>
	<string name="proxyServer">10.0.0.172</string>
	<string name="proxyPort">80</string>
	<string name="CMDipForwardEnable">echo 1 &gt; /proc/sys/net/ipv4/ip_forward
	</string>
	<string name="CMDipForwardDisable">echo 0 &gt; /proc/sys/net/ipv4/ip_forward
	</string>
	<string name="CMDiptablesEnable">iptables -t nat -A OUTPUT -o rmnet0 -p tcp
		-d android.clients.google.com --dport 443 -j DNAT --to-destination
		127.0.0.1:7443｜iptables -t nat -A OUTPUT -o rmnet0 -p tcp --dport 80
		-j DNAT --to-destination 10.0.0.172:80｜iptables -t nat -A OUTPUT -o
		rmnet0 -p tcp --dport 1080 -j DNAT --to-destination 10.0.0.172:80
	</string>
	<string name="CMDiptablesDisable">iptables -t nat -F</string>
	<string name="CMDremount">mount -o rw,remount -t yaffs2
		/dev/block/mtdblock3 /system</string>
	
	<string name="UserAgent">biAji's wap channel</string>
	<string name="MENU_TEST">环境测试</string>
	<string name="MENU_SETTING">设置</string>
	<string name="MENU_ABOUT">关于</string><string name="TEST_ROOT">测试是否具有root权限</string>
	<string name="TEST_IPTABLES">测试是否有iptables支持</string>
	<string name="TEST_CMWAP">测试当前是否CMWAP连接</string>
	<string name="TEST_HTTPS">测试是否支持HTTPS代理</string>
	<string name="TEST_OTHER">测试是否支持HTTPS以外的协议代理</string>
